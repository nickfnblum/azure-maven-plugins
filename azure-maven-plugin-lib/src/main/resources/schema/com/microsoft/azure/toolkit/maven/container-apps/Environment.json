{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Environment",
	"description": "Azure Container App Environment",
	"type": "object",
	"properties": {
		"environmentName": {
			"description": "The name of your Azure Container App Environment",
			"type": "string",
			"pattern": "^(?!.*--)[a-z0-9]([-a-z0-9]*[a-z0-9])$",
			"minLength": 2,
			"maxLength": 60
		},
		"resourceGroup": {
			"description": "Name for Azure Resource Group",
			"type": "string",
			"pattern": "^(?!.*--)[a-zA-Z0-9\\.\\_\\-\\(\\)]+$",
			"minLength": 2,
			"maxLength": 90
		}
	}
}
